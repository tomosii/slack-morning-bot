{
    "builds": [
      {
        "src": "app/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app/main.py"
      }
    ],
    "crons": [
      {
      
        "path": "/cron",
        "schedule": "*/5 * * * 1-5"
      }
    ]
  }